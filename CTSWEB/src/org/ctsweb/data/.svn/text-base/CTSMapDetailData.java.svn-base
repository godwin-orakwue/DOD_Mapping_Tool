package org.ctsweb.data;

/**
 * Map detail data.
 * 
 * @author wellerk
 *
 */
public class CTSMapDetailData {

    /**
     * From OID.
     */
    private String fromOid;
    
    /**
     * From code.
     */
    private String fromCode;
    
    /**
     * From description.
     */
    private String fromDescription;
    
    /**
     * To OID.
     */
    private String toOid;
    
    /**
     * To code.
     */
    private String toCode;
    
    /**
     * To description.
     */
    private String toDescription;
    
    /**
     * Active flag.
     */
    private boolean active = true;
    
    /**
     * Constructor.
     * 
     * @param mapdata data
     */
    public CTSMapDetailData(CTSMapDetailData mapdata) {
        super();
        this.fromOid = mapdata.fromOid;
        this.fromCode = mapdata.fromCode;
        this.fromDescription = mapdata.fromDescription;
        this.toOid = mapdata.toOid;
        this.toCode = mapdata.toCode;
        this.toDescription = mapdata.toDescription;
        this.active = mapdata.active;
    }

    /**
     * Constructor.
     */
    public CTSMapDetailData() {
        super();
    }

    /**
     * Get from OID.
     * 
     * @return the fromOid
     */
    public String getFromOid() {
        return fromOid;
    }
    
    /**
     * Set from OID.
     * 
     * @param fromOid the fromOid to set
     */
    public void setFromOid(String fromOid) {
        this.fromOid = fromOid;
    }
    
    /**
     * Get from code.
     * 
     * @return the fromCode
     */
    public String getFromCode() {
        return fromCode;
    }
    
    /**
     * Set from code.
     * 
     * @param fromCode the fromCode to set
     */
    public void setFromCode(String fromCode) {
        this.fromCode = fromCode;
    }
    
    /**
     * Get from description.
     * 
     * @return the fromDescription
     */
    public String getFromDescription() {
        return fromDescription;
    }
    
    /**
     * Set from description.
     * 
     * @param fromDescription the fromDescription to set
     */
    public void setFromDescription(String fromDescription) {
        this.fromDescription = fromDescription;
    }
    
    /**
     * Get to OID.
     * 
     * @return the toOid
     */
    public String getToOid() {
        return toOid;
    }
    
    /**
     * Set to OID.
     * 
     * @param toOid the toOid to set
     */
    public void setToOid(String toOid) {
        this.toOid = toOid;
    }
    
    /**
     * Get to code.
     * 
     * @return the toCode
     */
    public String getToCode() {
        return toCode;
    }
    
    /**
     * Set to code.
     * 
     * @param toCode the toCode to set
     */
    public void setToCode(String toCode) {
        this.toCode = toCode;
    }
    
    /**
     * Get to description.
     * 
     * @return the toDescription
     */
    public String getToDescription() {
        return toDescription;
    }
    
    /**
     * Set to description.
     * 
     * @param toDescription the toDescription to set
     */
    public void setToDescription(String toDescription) {
        this.toDescription = toDescription;
    }
    
    /**
     * Is active.
     * 
     * @return the active
     */
    public boolean isActive() {
        return active;
    }
    
    /**
     * Set active.
     * 
     * @param active the active to set
     */
    public void setActive(boolean active) {
        this.active = active;
    }
}
